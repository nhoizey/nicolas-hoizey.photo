---
layout: page
---

{{ content | safe }}

{% from "macros/photo.njk" import photo %}

<ul class="gallery">
  {% for item in collections.all_photos %}
    {% if loop.index < 4 %}
      <li>{{ photo(item, "eager") }}</li>
    {% else %}
      <li>{{ photo(item, "lazy") }}</li>
    {% endif %}
  {% endfor %}
</ul>
