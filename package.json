{
  "name": "nho-photo",
  "version": "1.0.0-alpha.1",
  "title": "Nicolas Hoizey photo",
  "description": "The photography portfolio of Nicolas Hoizey",
  "homepage": "https://nicolas-hoizey.photo/",
  "author": {
    "name": "Nicolas Hoizey",
    "url": "https://nicolas-hoizey.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhoizey/nicolas-hoizey.photo.git"
  },
  "bugs": {
    "url": "https://github.com/nhoizey/nicolas-hoizey.photo/issues"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf _site",
    "sass": "sass --no-source-map assets/sass:_site/ui/css",
    "postcss": "postcss _site/ui/css --no-map --replace --verbose",
    "build:sass": "npm-run-all --sequential sass postcss",
    "watch:sass": "sass --source-map-urls=absolute --watch assets/sass:_site/ui/css",
    "build:js": "rollup -c",
    "watch:js": "rollup -c --watch",
    "build:eleventy": "eleventy",
    "watch:eleventy": "eleventy --serve",
    "start": "cross-env NODE_ENV=development npm-run-all --sequential clean build:js --parallel watch:*",
    "debug:start": "DEBUG=Eleventy* npm start",
    "build": "cross-env NODE_ENV=production npm-run-all --sequential clean --parallel build:js build:sass --sequential build:eleventy",
    "debug:build": "DEBUG=Eleventy* npm run build",
    "sprite": "node ./_scripts/generate-svg-sprite.js"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.13.9",
    "@jlengstorf/get-share-image": "^0.8.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@sindresorhus/slugify": "~1.1",
    "autoprefixer": "~10.2.4",
    "babel-loader": "^8.2.2",
    "cheerio": "~1.0.0-rc.9",
    "cross-env": "^7.0.3",
    "cssnano": "~5.0.5",
    "dotenv": "^8.2.0",
    "eleventy-plugin-images-responsiver": "^1.8.2",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.3.1",
    "fast-glob": "^3.2.5",
    "feather-icons": "~4.28.0",
    "html-entities": "^2.3.2",
    "html-minifier": "^4.0.0",
    "image-size": "^0.9.4",
    "lodash": "^4.17.21",
    "majesticons": "~1.1.2",
    "markdown-it": "~12.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "~7.1",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-container": "~3.0",
    "markdown-it-footnote": "~3.0.3",
    "moment": "~2.29.1",
    "node-fetch": "^2.6.1",
    "npm-run-all": "~4.1.5",
    "path": "^0.12.7",
    "postcss-cli": "~8.3.1",
    "prettier": "2.2.1",
    "require-dir": "^1.2.0",
    "res": "~0.4.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sanitize-html": "^2.3.3",
    "sass": "~1.34.0",
    "simple-icons": "~5.0.0",
    "svgstore": "*",
    "truncate-html": "^1.0.3",
    "twitter-text": "^3.1.0"
  }
}
