{
  "name": "nho-photo",
  "version": "1.0.0",
  "title": "Nicolas Hoizey photo",
  "description": "The photography portfolio of Nicolas Hoizey",
  "homepage": "https://nicolas-hoizey.photo/",
  "author": {
    "name": "Nicolas Hoizey",
    "url": "https://nicolas-hoizey.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhoizey/nicolas-hoizey.photo.git"
  },
  "bugs": {
    "url": "https://github.com/nhoizey/nicolas-hoizey.photo/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16"
  },
  "browserslist": [
    "> 0.1%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not ie 11"
  ],
  "scripts": {
    "clean": "rimraf _site",
    "sass": "sass --no-source-map assets/sass:_site/ui/css",
    "postcss": "postcss _site/ui/css --no-map --replace --verbose",
    "build:sass": "npm-run-all --sequential sass postcss",
    "watch:sass": "sass --source-map-urls=absolute --watch assets/sass:_site/ui/css",
    "build:js": "rollup -c",
    "watch:js": "rollup -c --watch",
    "build:eleventy": "eleventy",
    "watch:eleventy": "eleventy --serve",
    "start": "cross-env NODE_ENV=development npm-run-all --sequential clean build:js --parallel watch:*",
    "debug:start": "DEBUG=Eleventy* npm start",
    "build": "cross-env NODE_ENV=production npm-run-all --sequential clean --parallel build:js build:sass --sequential build:eleventy",
    "debug:build": "DEBUG=Eleventy* npm run build",
    "subset": "glyphhanger https://nicolas-hoizey.photo/tools/subset --whitelist=U+0020 --subset=./assets/font/*.ttf --formats=woff2,woff --output=./src/ui/fonts/",
    "matomo": "node ./_scripts/update-matomo.js",
    "dates": "./_scripts/add-dates.sh",
    "prepare": "npx husky install",
    "test": "npx playwright test",
    "docker": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.22.0-focal /bin/bash"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@playwright/test": "^1.27.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@sindresorhus/slugify": "^1.1.2",
    "@turf/turf": "^6.5.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.14",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.3",
    "download": "^8.0.0",
    "eleventy-plugin-auto-preload": "^1.2.0",
    "eleventy-plugin-images-responsiver": "^1.12.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "exifr": "^7.1.3",
    "fast-glob": "^3.2.12",
    "feather-icons": "^4.29.0",
    "fraction.js": "^4.2.0",
    "git-pull-run": "^1.0.2",
    "glyphhanger": "^5.0.0",
    "gray-matter": "^4.0.3",
    "html-entities": "^2.3.3",
    "html-minifier": "^4.0.0",
    "husky": "^8.0.1",
    "image-size": "^1.0.2",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "majesticons": "^2.1.2",
    "mapbox-gl-style-switcher": "^1.0.11",
    "markdown-it": "^13.0.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.6.5",
    "markdown-it-attrs": "^4.1.4",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.3",
    "mbsprite": "^0.2.0",
    "moment": "^2.29.4",
    "node-fetch": "^2.6.7",
    "node-vibrant": "^3.2.1-alpha.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "pixelmatch": "^5.3.0",
    "playwright": "^1.27.1",
    "pngjs": "^6.0.0",
    "polylabel": "^1.1.0",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "postcss-fail-on-warn": "^0.2.1",
    "postcss-logical": "^5.0.4",
    "prettier": "^2.7.1",
    "puppeteer-cluster": "^0.23.0",
    "puppeteer-core": "^15.5.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-bundle-hash": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-entrypoint-hashmanifest": "^0.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.55.0",
    "sharp": "^0.30.7",
    "simple-icons": "^6.23.0",
    "svg-country-flags": "^1.2.10",
    "svgstore": "^3.0.1",
    "truncate-html": "^1.0.4",
    "twitter-text": "^3.1.0",
    "utf8": "^3.0.0",
    "yaml": "^1.10.2"
  },
  "dependencies": {
    "mapbox-gl": "^2.10.0",
    "web-vitals": "^2.1.4"
  }
}
